	    #include<bits/stdc++.h>
	    using namespace std;
	     
	    int main(){
	    	long long n,m;
	    	cin>>n>>m;
	    	bool flag=true,flag1=true;
	    	while(n<m){
	    		n+=1;
	    		long long i=2;
	    		for(i=2;i<n;i++){
	    			if(n%i==0){
	    				flag1=false;
	    				break;
	    			}
	    		}
	    		if(flag1==true){
	    			if(i==m)
	    				cout<<"YES";
	    			else 
	    				cout<<"NO";
	    			return 0;
	    		}
	    		flag1=true;
	    	}
	    	cout<<"NO";
	    	return 0;
	    }
