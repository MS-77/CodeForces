    #include<bits/stdc++.h>
    using namespace std;
     
    int main(){
    	int n;
    	cin>>n;
    	std::map<int , int> mp;
    	int l1;
    	cin>>l1;
    	for(int i=0;i<l1;i++){
    		int s;
    		cin>>s;
    		mp[s]+=1;
    	}
    	int l2;
    	cin>>l2;
    	for(int i=0;i<l2;i++){
    		int s;
    		cin>>s;
    		mp[s]+=1;
    	}
    	for(int i=0;i<n;i++){
    		if(!mp[i+1]){
    			cout<<"Oh, my keyboard!";
    			return 0;
    		}
    	}
    	cout<<"I become the guy.";
    	return 0;
    }
