    #include<bits/stdc++.h>
    using namespace std;
     
    int main(){
    	unsigned long long n,x;
    	cin>>n>>x;
    	if(n==1&&x==10){
    		cout<<-1;
    		return 0;
    	}else if(x==10&&n>1){
    		n--;
    		cout<<1;
    		while(n--){
    			cout<<0;
    		}
    		return 0;
    	}
    	while(n--){
    		cout<<x;
    	}
    	return 0;
    }
