    #include<bits/stdc++.h>
    using namespace std;
     
    int main(){
    	long long n,m;
    	cin>>n>>m;
    	char a[n][m];
    	for(long long i=0;i<n;i++){
    		for(long long j=0;j<m;j++){
    			cin>>a[i][j];
    		}
    	}
    	for(long long i=0;i<n;i++){
    		for(long long j=0;j<m;j++){
    			if(a[i][j]=='C'||a[i][j]=='M'||a[i][j]=='Y'){
    				cout<<"#Color";
    				return 0;
    			}
    		}
    	}
    	cout<<"#Black&White";
    	return 0;
    }
