#include <bits/stdc++.h>
using namespace std;

int main(){
	string s1="",s2="";
	cin>>s1;
	cin>>s2;
	std::transform (s1.begin(), s1.end(), s1.begin(), ::tolower);
	std::transform (s2.begin(), s2.end(), s2.begin(), ::tolower);

	if(s2==s1)
		cout<<"0";
	else if(s1>s2)
		cout<<"1";
	else
		cout<<"-1";

	return 0;
}
