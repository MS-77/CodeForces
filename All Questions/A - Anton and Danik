#include<bits/stdc++.h>
using namespace std;

int main(){
	int n;
	cin>>n;
	string s;
	cin>>s;
	string a="Anton",d="Danik",f="Friendship";
	int counta=0,countb=0;
	for(int i=0;i<s.size();i++){
		if(s[i]=='A')
			counta++;
		else
			countb++;
	}
	if(countb<counta)
		cout<<a;
	else if(counta<countb)
		cout<<d;
	else
		cout<<f;
	return 0;
}
